{
  "name": "active-expressions-benchmark",
  "version": "1.0.0",
  "description": "Benchmarking the 3 implementation strategies of active-expressions",
  "main": "index.js",
  "scripts": {
    "test": "npm test",
    "build:bench": "rollup -c rollup.config.js",
    "build-baseline": "rollup -c rollup.config.baseline.js",
    "build-ticking": "rollup -c rollup.config.ticking.js",
    "build-rewrite": "rollup -c rollup.config.rewrite1.js && cd benchmark/temp && babel rewriting_aexpr.js --out-file rewriting_babel_out.js && cd ../.. && rollup -c rollup.config.rewrite2.js",
    "build-interpretation": "rollup -c rollup.config.interpretation.js",
    "watch": "chokidar benchmark/tests/**/* -c \"rollup -c rollup.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/active-expressions/active-expressions-benchmark.git"
  },
  "keywords": [
    "active"
  ],
  "author": "stefan.ramson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/active-expressions/active-expressions-benchmark/issues"
  },
  "homepage": "https://github.com/active-expressions/active-expressions-benchmark#readme",
  "dependencies": {
    "active-expressions": "^1.5.0",
    "aexpr-source-transformation-propagation": "^1.2.1",
    "babel-cli": "^6.11.4",
    "babel-plugin-aexpr-source-transformation": "^2.1.3",
    "babel-plugin-locals": "^1.0.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-es2015-rollup": "^1.2.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "floom-mpm": "^1.0.0",
    "mocha": "^3.0.2",
    "random-seed": "^0.3.0",
    "rollem": "^1.2.0",
    "rollup": "^0.34.8",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^3.3.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-watch": "^2.5.0",
    "stack-es2015-modules": "^1.1.0"
  }
}
